{
  "manifest_version": 3,
  "name": "Assistente de Consultas USF",
  "version": "1.2",
  "description": "Acesso rápido a protocolos e geração de texto SOAP em um painel lateral.",
  "permissions": ["sidePanel", "clipboardWrite"],
  "host_permissions": ["https://www2.bauru.sp.gov.br/*"],
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "action": {
    "default_title": "Abrir Assistente de Consultas"
  }
}
